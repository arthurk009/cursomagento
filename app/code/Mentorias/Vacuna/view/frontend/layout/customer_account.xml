<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="
            customer-account-navigation-registro">
                <arguments>
                    <argument name="path" xsi:type="string">vacuna/index/step1</argument>
                    <argument name="label" xsi:type="string">Registro Vacunas</argument>
                </arguments>
            </block>
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="
            customer-account-navigation-list">
                <arguments>
                    <argument name="path" xsi:type="string">vacuna/index/vacunaslist</argument>
                    <argument name="label" xsi:type="string">Listado de vacunas</argument>
                </arguments>
            </block>
        </referenceBlock>

    </body>
</page>